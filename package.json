{
  "name": "@backtrack/preset-style",
  "version": "0.1.2",
  "description": "backtrack preset for linting / styling node applications",
  "keywords": [
    "backtrack",
    "preset",
    "style",
    "eslint",
    "prettier",
    "lint",
    "format"
  ],
  "license": "ISC",
  "author": "Chris Blossom",
  "repository": "chrisblossom/backtrack-preset-style",
  "homepage": "https://github.com/chrisblossom/backtrack-preset-style",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.10",
    "yarn": ">=1.6.0"
  },
  "files": [
    "lib/"
  ],
  "main": "lib/preset-style.js",
  "scripts": {
    "format": "backtrack format",
    "lint": "backtrack lint",
    "lint.fix": "backtrack lint.fix",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run lint",
    "prepush": "npm run lint"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "backtrack": {
    "presets": [
      "@backtrack/preset"
    ]
  },
  "devDependencies": {
    "@backtrack/core": "^0.1.11",
    "@backtrack/preset-preset": "^0.1.5"
  },
  "dependencies": {
    "@chrisblossom/eslint-config": "^2.0.7",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1"
  }
}
